    <print name="RG_MLP80A">
        <mlp_receive>! %@ %@ %@ %@ %@</mlp_receive>
        
        <mlp_pagewidth>PAGE-WIDTH %@</mlp_pagewidth>
        
        <mlp_text>TEXT %@ %@ %@ %@ %@</mlp_text>
        
        <mlp_1d>B %@ %@ %@ %@ %@ %@ %@</mlp_1d>
        <!-- BARCODE-TEXT  字体  大小  偏移-->
        <mlp_1d_text>BARCODE-TEXT %@ %@ %@</mlp_1d_text>
        
        <mlp_1d_off>BARCODE-TEXT OFF</mlp_1d_off>
       	
        <mlp_line>LINE %@ %@ %@ %@ %@</mlp_line>
        
        <mlp_box>BOX %@ %@ %@ %@ %@</mlp_box>
        
        <mlp_location>FORM</mlp_location>
        
        <mlp_endprint>PRINT</mlp_endprint>
        
        <mlp_2d>BARCODE %@ %@ %@ M %@ U %@</mlp_2d>
        
        <mlp_2data>%@</mlp_2data>
        
        <mlp_data>MA,%@</mlp_data>
        
        <mlp_endqr>ENDQR</mlp_endqr>
        
        <mlp_gap>GAP-SENSE</mlp_gap>
        <!-- EG 图形指令，指令分别是宽（单位：横向字节数）高（纵向点数）x轴（位置点数）y轴（位置点数） 数据（字符串格式）-->
        <mlp_graphice_eg>EG %@ %@ %@ %@ %@</mlp_graphice_eg>
        <!-- +++++++++++ -->
        <mlp_blod>SETBOLD %@</mlp_blod>
        
        <mlp_lt>INVERSE-TEXT %@</mlp_lt>
        
        <mlp_gaplt>INVERSE-LINE %@ %@ %@ %@ %@</mlp_gaplt>
        
        <mlp_ut>UNDERLINE-TEXT %@</mlp_ut>
        
        <mlp_space>SETSP %@</mlp_space>
        
        <mlp_center>CENTER</mlp_center>
        
        <mlp_left>LEFT</mlp_left>
        
        <mlp_right>RIGHT</mlp_right>
        
        <mlp_prefeed>PREFEED %@</mlp_prefeed>
        
        <mlp_postfeed>POSTFEED %@</mlp_postfeed>
    </print>
